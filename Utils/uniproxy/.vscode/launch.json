    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "BIND_ADDR": "127.0.0.1:8080",
                "LOG_LEVEL": "debug",
                "CONFIG_FILE": "etc/uniproxy/uniproxy.yaml"
            },
        },
        {
            "name": "Run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "BIND_ADDR": "127.0.0.1:8080",
                "LOG_LEVEL": "info",
                "CONFIG_FILE": "etc/uniproxy/uniproxy.yaml"
            },
        }
    ]
}